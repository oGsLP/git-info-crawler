	"main": "index.js",
	"keywords": [
		"crawler",
		"git"
	],
	"devDependencies": {
		"coveralls": "^3.0.2",
		"eslint": "^5.9.0",
		"eslint-config-prettier": "^3.3.0",
		"eslint-config-xo": "^0.25.0",
		"eslint-plugin-prettier": "^3.0.0",
		"husky": "^1.2.0",
		"jest": "^23.5.0",
		"lint-staged": "^8.1.0",
		"prettier": "^1.15.2"
	},
	"engines": {
		"npm": ">= 4.0.0"
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		],
		"*.json": [
			"prettier --write",
			"git add"
		]
	},
	"eslintConfig": {
		"extends": [
			"xo",
			"prettier"
		],
		"env": {
			"jest": true,
			"node": true
		},
		"rules": {
			"prettier/prettier": "error",
			"camelcase": "off"
		},
		"plugins": [
			"prettier"
		]
	},
	"scripts": {
		"pretest": "eslint .",
		"test": "jest"
	},
	"repository": "oGsLP/git-info-crawler",
	"license": "MIT",
	"dependencies": {
		"axios": "^0.18.0",
		"chalk": "^2.4.2",
